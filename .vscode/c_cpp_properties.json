{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/inc",
                "${workspaceFolder}/src"
            ],
            "defines": [
                "_POSIX_C_SOURCE=200809L",
                "HAVE_MMAP=0",
                "HAVE_MORECORE=0",
                "USE_LOCKS=0",
                "MSPACES=1",
                "FOOTERS=0",
                "INSECURE=0",
                "NO_MALLOC_STATS=1",
                "MMAP_CLEARS=0"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c99",
            "cppStandard": "c++23",
            "intelliSenseMode": "linux-clang-x64"
        }
    ],
    "version": 4
}